for i in range(len(nilaiMhs)):
    mhs=dict(nilaiMhs[i])
    nilaifinal = ((mhs['uas']*2 + mhs['mid'])/3)
    del mhs['uas']
    del mhs['mid']
    mhs['nilaiakhir']=nilaifinal
    print(mhs)
